{
    "name": "ganbarodigital/php-the-missing-bits",
    "type": "library",
    "license": "BSD-3-Clause",
    "authors": [
        { "name": "Stuart Herbert", "email": "stuart@stuartherbert.com" }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/stuartherbert/Couscous"
        }
    ],
    "require": {
        "php": ">= 5.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0",
        "couscous/couscous": "dev-develop"
    },
    "autoload": {
        "psr-4": {
            "GanbaroDigital\\MissingBits\\": "src"
        },
        "files": [
            "src/array-functions.php",
            "src/class-object-functions.php",
            "src/console-functions.php",
            "src/error-shim.php",
            "src/list-functions.php",
            "src/string-functions.php",
            "src/trace-functions.php",
            "src/type-functions.php"
        ]
    }
}
